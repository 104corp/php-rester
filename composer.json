{
    "name": "104corp/rester",
    "description": "A library for calling REST API.",
    "type": "library",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/104corp/php-rester/issues"
    },
    "require": {
        "php": "^7.1",
        "104corp/support": "^1.2",
        "guzzlehttp/guzzle": "^6.3",
        "psr/http-message": "^1.0"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.9",
        "mockery/mockery": "^0.9",
        "phpunit/phpunit": "^7 | ^8",
        "slim/slim": "^3.9",
        "squizlabs/php_codesniffer": "^3.5",
        "zendframework/zend-diactoros": "<1.5"
    },
    "autoload": {
        "psr-4": {
            "Corp104\\Rester\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    }
}
